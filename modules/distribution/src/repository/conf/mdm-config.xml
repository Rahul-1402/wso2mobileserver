<wso2mobile-mdm>

	<ios-mdm-conf>
		<!-- ios mdm endpoint urls -->
		<ios-enroll-url>https://192.168.1.157:9443/mdm/scep</ios-enroll-url>
		<ios-profile-url>https://192.168.1.157:9443/mdm/profile</ios-profile-url>
		<ios-checkin-url>https://192.168.1.157:9443/mdm/checkin</ios-checkin-url>
		<ios-server-url>https://192.168.1.157:9443/mdm/server</ios-server-url>
		
		<!-- ios mdm APNS configurations -->
		<ios-apns-cert-path>E:/Mobile/iOS_MDM_Impl/keys/PlainCert.pfx</ios-apns-cert-path>
		<ios-apns-cert-password>shan130474</ios-apns-cert-password>
		
		<!-- use carbon.xml entries later -->
		<keystore>
            <!-- Keystore file location-->
            <keystore-location>E:/Mobile/iOS_MDM_Impl/generated_keys_openssl/keystore.jks</keystore-location>
            <!-- Keystore type (JKS/PKCS12 etc.)-->
            <keystore-type>JKS</keystore-type>
            <!-- Keystore password-->
            <keystore-password>123456</keystore-password>
            <!-- Private Key alias-->
            <keystore-key-alias>1</keystore-key-alias>
            <!-- Private Key password-->
            <keystore-key-password>123456</keystore-key-password>
			
			<!-- below configurations will be moved to the keystore later -->
			<ca-cert-path>E:/Mobile/iOS_MDM_Impl/keys/ca_cert.pem</ca-cert-path>
			<ca-privateky-key-path>E:/Mobile/iOS_MDM_Impl/keys/ca_private.pem</ca-privateky-key-path>
			<ra-cert-path>E:/Mobile/iOS_MDM_Impl/keys/ra_cert.pem</ra-cert-path>
			<ra-privateky-key-path>E:/Mobile/iOS_MDM_Impl/keys/ra_private.pem</ra-privateky-key-path>
        </keystore>
	</ios-mdm-conf>
	
</wso2mobile-mdm>


